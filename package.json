{
  "name": "hackerrank-test-npm",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "jest-junit": "^10.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.4",
    "react-scripts": "3.3.1",
    "redux": "^4.1.0",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "HOST=0.0.0.0 PORT=8000 ./node_modules/.bin/react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "testw": "react-scripts test --watchAll",
    "testcov": "react-scripts test --coverage --updateSnapshot",
    "test-hackerrank": "npm install && rm -f ./junit.xml && ./node_modules/.bin/react-scripts test --verbose --env=jsdom --testResultsProcessor ./node_modules/jest-junit",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "8.0.0 - 8.17.0 || 10.0.0 - 10.19.0 || 12.0.0 - 12.14.1 || 13.0.0 - 13.7.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.19.0"
  }
}
